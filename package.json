{
  "name": "open-adblocker",
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "build:dev:chrome": "webpack --config webpack/webpack.config.js --mode=development --env BROWSER=chrome",
    "build:prod:chrome": "webpack --config webpack/webpack.config.js --mode=production --env BROWSER=chrome",
    "build:dev:edge": "webpack --config webpack/webpack.config.js --mode=development --env BROWSER=edge",
    "build:prod:edge": "webpack --config webpack/webpack.config.js --mode=production --env BROWSER=edge",
    "build:dev:all": "npm run build:dev:chrome && npm run build:dev:edge",
    "build:prod:all": "npm run build:prod:chrome && npm run build:prod:edge",
    "sync-rules": "tsx tools/sync-rules.ts",
    "test": "jest",
    "lint": "npx eslint ./",
    "lint:fix": "npx eslint ./ --fix",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "jest --coverage --no-cache",
    "prepare": "husky"
  },
  "dependencies": {
    "@adguard/agtree": "^3.2.1",
    "@adguard/tsurlfilter": "^3.4.0",
    "@adguard/tswebextension": "^3.2.4",
    "inversify": "^7.5.4",
    "pinia": "^3.0.3",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.1.0",
    "vue": "^3.5.17",
    "vue-inline-svg": "^3.1.2",
    "vue-router": "^4.5.1"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@adguard/dnr-rulesets": "^3.0.20250626120110",
    "@babel/core": "^7.28.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/runtime": "^7.27.0",
    "@eslint/js": "^9.31.0",
    "@types/chrome": "^0.0.329",
    "@types/jest": "^29.5.10",
    "@types/node": "^24.0.10",
    "@types/webpack-env": "^1.18.8",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.5",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.1",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^10.3.0",
    "filemanager-webpack-plugin": "^8.0.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "less": "^4.3.0",
    "less-loader": "^12.3.0",
    "openai": "4.26.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vue-loader": "^17.4.2",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  }
}
